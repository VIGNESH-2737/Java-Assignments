class Main {
    public static void main(String[] args) {
        int n = 5;
        int[] arr = new int[n-1];
        try{
            for(int i=0; i< n; i++){
                System.out.println(arr[i]);
            }
        }catch(ArrayIndexOutOfBoundsException e){
            System.out.println("Index of Out Bound Handled");
        }
    }
}
