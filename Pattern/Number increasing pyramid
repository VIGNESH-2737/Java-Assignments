
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();
        //int n = 6;
        for(int i=0; i<n; i++){
            for(int j=0; j<= i; j++){
                    System.out.print(j+1+" ");
            }
            System.out.println();
        }

    scan.close();

    }
}
