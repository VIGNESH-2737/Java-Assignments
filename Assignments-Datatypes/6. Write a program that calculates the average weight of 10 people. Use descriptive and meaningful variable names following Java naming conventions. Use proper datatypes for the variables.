import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        double average=0;
        
        for(int i=0; i<10; i++){
            average += scan.nextDouble();
        }
        
        average = average/10;
        
        System.out.print(average);
    }
}
